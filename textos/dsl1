entity(empregado, nome, key cpf, tipo_empregado, crea)
entity(departamento, nome, key id)
entity(projeto, key id, nome)
entity(ata_de_reuniao, data, key id)
entity(secretario, tipo)
entity(engenheiro, crea)
specialization(empregado, secretario, engenheiro)
relation(lotacao, empregado [0:N], departamento [1:1])
relation(registra, secretario [1:1], ata_de_reuniao [0:N])
relation(participacao, engenheiro [0:N], projeto [0:N])
attribute(participacao, funcao)